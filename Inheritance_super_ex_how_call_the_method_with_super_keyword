package Day_6;
class Codetantra{
	public void greeting(String name) {
		System.out.println("Hello "+name+" ,welcome to codetantra");
	}
}
class Student extends Codetantra{
	public void register(String name) {
		System.out.println(name+" your registration is complete");
		super.greeting(name);
	}
}

public class Inheritance_super_ex_how_call_the_method_with_super_keyword {

	public static void main(String[] args) {
		Student s1=new Student();
		String name=args[0];
		s1.register(name);

	}

}
